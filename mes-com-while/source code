#include <stdio.h>
#include <stdbool.h>
int main()
{
       int mes = 1;
   while(mes >=1 && mes<= 12)
   {
       
      printf("%i - Janeiro \n", mes);
      mes++;
       printf("%i - Fevereiro \n", mes);
      mes++;
       printf("%i - Marco \n", mes);
      mes++;
       printf("%i - Abril \n", mes);
      mes++;
       printf("%i - Maio \n", mes);
      mes++;
       printf("%i - Junho \n", mes);
      mes++;
       printf("%i - Julho \n", mes);
      mes++;
       printf("%i - Agosto \n", mes);
      mes++;
       printf("%i - Setembro \n", mes);
      mes++;
       printf("%i - Outubro \n", mes);
      mes++;
    printf("%i - Novembro \n", mes);
      mes++;
    printf("%i - Dezembro \n", mes);
      mes++;
   
   }
}